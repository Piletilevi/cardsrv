PROJECTS = DrvFRTst.exe DrvFRTst1C.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DrvFRTst.exe: DrvFRTst\DrvFRTst.dpr
  $(DCC)

DrvFRTst1C.exe: DrvFRTst1C\DrvFRTst1C.dpr
  $(DCC)


